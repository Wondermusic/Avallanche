<?xml version="1.0" encoding="utf-8"?>
<Window xmlns:x="http://www.w3.org/2001/XMLSchema-instance" x:type="PlayerWindow">
  <IsDefault>false</IsDefault>
  <Width>1024</Width>
  <Height>768</Height>
  <Name>RockstarPlaying</Name>
  <Id>30</Id>
  <BackgroundBrush />
  <WindowAnimations />
  <WindowControls>
    <Control x:type="Group">
      <Name>Playing Screen</Name>
      <Id>1</Id>
      <Width>1024</Width>
      <Height>768</Height>
      <VisibleCondition />
      <Animations />
      <MediaPortalFocusControls />
      <GroupStyle StyleId="Empty" />
      <GroupControls>
        <Control x:type="Group">
          <Name>Background</Name>
          <Id>2</Id>
          <Width>1024</Width>
          <Height>768</Height>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <GroupStyle>
            <BackgroundBrush />
            <BorderBrush />
            <GlossBrush />
          </GroupStyle>
          <GroupControls>
            <Control x:type="Image">
              <Name>Image</Name>
              <Id>3</Id>
              <Width>1024</Width>
              <Height>768</Height>
              <VisibleCondition />
              <Animations />
              <MediaPortalFocusControls />
              <ImageStyle StyleId="Logo_Image" />
              <Image>#MPD2.fanart.music</Image>
              <DefaultImage>\BG\hover_spotifier.jpg</DefaultImage>
              <ImageStretch>Fill</ImageStretch>
              <CoverCornerRadius>0,0,0,0</CoverCornerRadius>
            </Control>
            <Control x:type="Image">
              <Name>Overlay</Name>
              <Id>4</Id>
              <Width>1024</Width>
              <Height>768</Height>
              <VisibleCondition />
              <Animations />
              <MediaPortalFocusControls />
              <ImageStyle StyleId="FilmStrip Fanart Overlay" />
              <ImageStretch>Fill</ImageStretch>
              <CoverCornerRadius>0,0,0,0</CoverCornerRadius>
            </Control>
            <Control x:type="Group">
              <Name>WeatherTime</Name>
              <Id>79</Id>
              <Width>1024</Width>
              <Height>85</Height>
              <VisibleCondition />
              <Animations />
              <MediaPortalFocusControls />
              <GroupStyle StyleId="Empty" />
              <GroupControls>
                <Control x:type="Image">
                  <Name>MP Image</Name>
                  <Id>80</Id>
                  <Width>78</Width>
                  <Height>57</Height>
                  <PosX>10</PosX>
                  <PosY>10</PosY>
                  <VisibleCondition />
                  <Animations />
                  <MediaPortalFocusControls />
                  <ImageStyle StyleId="Logo_Image" />
                  <Image>\Styles\White\MP-Logo.png</Image>
                  <ImageStretch>Uniform</ImageStretch>
                  <CoverCornerRadius>0,0,0,0</CoverCornerRadius>
                </Control>
                <Control x:type="Rectangle">
                  <Name>Devider</Name>
                  <Id>81</Id>
                  <Width>3</Width>
                  <Height>73</Height>
                  <PosX>100</PosX>
                  <PosY>2</PosY>
                  <VisibleCondition />
                  <Animations />
                  <MediaPortalFocusControls />
                  <BackgroundBrush x:type="ColorBrush" Color="#FF6AA5CB" />
                </Control>
                <Control x:type="Label">
                  <Name>Window</Name>
                  <Id>82</Id>
                  <Width>593</Width>
                  <Height>60</Height>
                  <PosX>109</PosX>
                  <PosY>8</PosY>
                  <VisibleCondition />
                  <Animations />
                  <MediaPortalFocusControls />
                  <LabelStyle StyleId="Avalon 50 Blue" />
                  <LabelText>#currentmodule+ / +#Rockstar.PluginName</LabelText>
                  <IsScrollingEnabled>false</IsScrollingEnabled>
                  <ScrollSpeed>2</ScrollSpeed>
                  <IsScrollWrapEnabled>false</IsScrollWrapEnabled>
                </Control>
              </GroupControls>
            </Control>
          </GroupControls>
        </Control>
        <Control x:type="Image">
          <Name>Image</Name>
          <Id>10</Id>
          <Width>1024</Width>
          <Height>450</Height>
          <PosY>395</PosY>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <ImageStyle StyleId="Music Details BG" />
          <ImageStretch>None</ImageStretch>
          <CoverCornerRadius>0,0,0,0</CoverCornerRadius>
        </Control>
        <Control x:type="Label">
          <Name>Artist</Name>
          <Id>11</Id>
          <Width>950</Width>
          <Height>95</Height>
          <PosX>35</PosX>
          <PosY>431</PosY>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <LabelStyle StyleId="Avalon 85 White" />
          <LabelText>#Play.Current.Artist</LabelText>
          <LabelTextAlignment>Center</LabelTextAlignment>
          <IsScrollingEnabled>false</IsScrollingEnabled>
          <ScrollSpeed>2</ScrollSpeed>
          <IsScrollWrapEnabled>false</IsScrollWrapEnabled>
        </Control>
        <Control x:type="Label">
          <Name>Title</Name>
          <Id>12</Id>
          <Width>950</Width>
          <Height>90</Height>
          <PosX>35</PosX>
          <PosY>518</PosY>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <LabelStyle StyleId="Avalon 74 White" />
          <LabelText>#Play.Current.Album+ - +#Play.Current.Title</LabelText>
          <LabelTextAlignment>Center</LabelTextAlignment>
          <IsScrollingEnabled>false</IsScrollingEnabled>
          <ScrollDelay>2</ScrollDelay>
          <ScrollSpeed>8</ScrollSpeed>
          <IsScrollWrapEnabled>false</IsScrollWrapEnabled>
          <ScrollSeperator>   |   </ScrollSeperator>
        </Control>
        <Control x:type="ProgressBar">
          <Name>ProgressBar</Name>
          <Id>13</Id>
          <Width>930</Width>
          <Height>50</Height>
          <PosX>45</PosX>
          <PosY>625</PosY>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <ProgressBarStyle StyleId="Default" />
          <ProgressValue>#percentage</ProgressValue>
        </Control>
        <Control x:type="Label">
          <Name>currentplaytime</Name>
          <Id>14</Id>
          <Width>350</Width>
          <Height>84</Height>
          <PosX>339</PosX>
          <PosY>675</PosY>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <LabelStyle StyleId="Avalon 74 White" />
          <LabelText>#currentplaytime</LabelText>
          <LabelTextAlignment>Center</LabelTextAlignment>
          <IsScrollingEnabled>false</IsScrollingEnabled>
          <ScrollSpeed>2</ScrollSpeed>
          <IsScrollWrapEnabled>false</IsScrollWrapEnabled>
        </Control>
        <Control x:type="Image">
          <Name>Cover</Name>
          <Id>15</Id>
          <Width>250</Width>
          <Height>250</Height>
          <PosX>391</PosX>
          <PosY>118</PosY>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <ImageStyle StyleId="Logo_Image" />
          <Image>#Play.Current.Thumb</Image>
          <DefaultImage>\audio_nothumb.png</DefaultImage>
          <ImageStretch>Uniform</ImageStretch>
          <ImageCornerRadius>15</ImageCornerRadius>
          <CoverCornerRadius>0,0,0,0</CoverCornerRadius>
        </Control>
      </GroupControls>
    </Control>
    <Control x:type="Group">
      <Name>TimeRec</Name>
      <Id>85</Id>
      <Width>1024</Width>
      <Height>80</Height>
      <VisibleCondition />
      <Animations />
      <MediaPortalFocusControls />
      <GroupStyle />
      <GroupControls>
        <Control x:type="Label">
          <Name>Time</Name>
          <Id>86</Id>
          <Width>308</Width>
          <Height>95</Height>
          <PosX>711</PosX>
          <PosY>-10</PosY>
          <VisibleCondition>!IsControlVisible(87)</VisibleCondition>
          <Animations>
            <Animation x:type="Fade" Condition="VisibleTrue" Duration="1000" />
            <Animation x:type="Fade" Condition="VisibleFalse" Duration="1000" Reverse="true" Repeat="100000" From="100" To="0" />
          </Animations>
          <MediaPortalFocusControls />
          <LabelStyle StyleId="Avalon 85 Grey" />
          <LabelText>#time</LabelText>
          <LabelTextAlignment>Right</LabelTextAlignment>
          <IsScrollingEnabled>false</IsScrollingEnabled>
          <ScrollSpeed>2</ScrollSpeed>
          <IsScrollWrapEnabled>false</IsScrollWrapEnabled>
        </Control>
        <Control x:type="Image">
          <Name>Rec Icon</Name>
          <Id>87</Id>
          <Width>200</Width>
          <Height>76</Height>
          <PosX>820</PosX>
          <VisibleCondition>IsTvRecording</VisibleCondition>
          <IsWindowOpenVisible>false</IsWindowOpenVisible>
          <Animations>
            <Animation x:type="Fade" Condition="VisibleTrue" Duration="1000" Reverse="true" Repeat="50000" />
            <Animation x:type="Fade" Condition="VisibleFalse" Duration="1000" From="100" To="0" />
          </Animations>
          <MediaPortalFocusControls />
          <ImageStyle StyleId="Logo_Image" />
          <Image>\RecordingIndicator.png</Image>
          <ImageStretch>Fill</ImageStretch>
          <CoverCornerRadius>0,0,0,0</CoverCornerRadius>
        </Control>
      </GroupControls>
    </Control>
    <Control x:type="Button">
      <Name>Play_Menu</Name>
      <Id>16</Id>
      <Width>1024</Width>
      <Height>150</Height>
      <PosY>615</PosY>
      <VisibleCondition />
      <Animations />
      <MediaPortalFocusControls />
      <ButtonStyle StyleId="Clear" />
      <LabelText>NewButton</LabelText>
      <Actions>
        <Action Action="OpenDialog" Param="904" />
      </Actions>
    </Control>
  </WindowControls>
  <VisibleCondition>IsPlayer(Rockstar)</VisibleCondition>
</Window>